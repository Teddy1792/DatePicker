{
  "name": "datepickerfromscratch",
  "version": "1.1.1",
  "private": false,
  "type": "module",
  "description": "The datepickerfromscratch is a custom date picker built for React applications using TypeScript.",
  "main": "./dist/datepickerfromscratch.umd.cjs",
  "module": "./dist/datepickerfromscratch.es.js",
  "exports": {
    ".": {
      "import": "./dist/datepickerfromscratch.es.js",
      "require": "./dist/datepickerfromscratch.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "react-select": "^5.8.0",
    "sass": "^1.69.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Teddy1792/DatePicker.git"
  },
  "homepage": "https://github.com/Teddy1792/DatePicker#readme",
  "keywords": [
    "react",
    "date picker",
    "calendar",
    "input",
    "form",
    "component",
    "accessibility",
    "TypeScript",
    "UI",
    "picker"
  ],
  "license": "MIT"
}

